
def import_pods
  pod 'BigInt'
  pod 'EthereumAddress'
  pod 'SwiftRLP', '~> 1.1'
  pod 'secp256k1_swift', '~> 1.0.3', :modular_headers => true
  pod 'CryptoSwift'
end

target 'web3Swift-Ethereum' do
  # Uncomment the next line if you're using Swift or would like to use dynamic frameworks
  use_frameworks!
  import_pods

  # Pods for web3Swift-Ethereum

end
